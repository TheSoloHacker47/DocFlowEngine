{
  "name": "docflowengine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "mkdir -p public/js && cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/js/pdf.worker.min.mjs",
    "perf:test": "node scripts/performance-test.js",
    "perf:lighthouse": "lhci autorun",
    "perf:audit": "lighthouse http://localhost:3000 --output html --output-path ./performance-reports/lighthouse-audit.html",
    "perf:ci": "npm run build && npm run start & npm run perf:lighthouse && kill %1"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^9.31.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "pdfjs-dist": "^4.10.38",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.5.0",
    "cypress-axe": "^1.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "lighthouse": "^12.6.1",
    "prettier": "^3.6.0",
    "puppeteer": "^24.10.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
